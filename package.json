{
  "name": "current",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:dry-run": "turbo run build --dry",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter=docs",
    "dev:web": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "upgrade": "pnpm up --recursive"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.8.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
